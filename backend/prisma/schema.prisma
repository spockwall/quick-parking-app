generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id                 String   @id @default(uuid())
  name               String
  password           String
  phone              String
  email              String
  licensePlateNumber String[]
  role               String
  status             String
}
