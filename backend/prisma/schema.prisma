// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model ParkingSpace {
  id                 Int       @id @default(autoincrement())
  parkingSpaceId     String    @unique
  state              String    // Enum type might be more appropriate, but using String here
  type               String    // Enum type might be more appropriate, but using String here
  startTime          Int?      // Nullable field
  occupant            Int?      // Nullable field (references User_id)
  licensePlateNumber String?   // Nullable field
  floor              Int
  lot                Int
}
