generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id                 String   @id
  name               String
  password           String
  phone              String
  email              String
  licensePlateNumber String[]
  role               Role
  status             Status
}

enum Role {
  staff
  admin
  guard
}

enum Status {
  common
  difficulty
  disability
}

model ParkingSpace {
  id                 Int       @id @default(autoincrement())
  SpaceId            String    @unique
  state              State
  type               Status
  startTime          Int?      
  occupant           String?     
  floor              Int
  slot               Int
  licensePlateNumber String?   
}

enum State{
  available
  occupied
  error
}