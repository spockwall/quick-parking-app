// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id                 String   @id @default(uuid())
  name               String
  password           String
  phone              String
  email              String
  licensePlateNumber String   // We'll store license plates as comma-separated values
  role               String
  status             String
}
